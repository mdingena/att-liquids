{
  "name": "att-liquids",
  "version": "1.0.0",
  "description": "Create custom liquids with ease.",
  "homepage": "https://github.com/mdingena/att-liquids#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdingena/att-liquids.git"
  },
  "keywords": [],
  "author": "Marc Dingena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdingena/att-liquids/issues"
  },
  "scripts": {
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky install",
    "prepare:build": "npm run build",
    "lint": "eslint --fix \"src/**/*.{ts,js}\"",
    "format": "prettier --write \"+(*.md|src/**/*.{js,ts,json,md})\"",
    "compile": "tsc --noEmit",
    "prebuild": "rimraf dist/",
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build:esm": "tsc -p tsconfig-esm.json",
    "postbuild:cjs": "node --loader ts-node/esm ./scripts/postbuild-cjs.ts",
    "test": "vitest run"
  },
  "engines": {
    "node": ">=20.4.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "att-string-transcoder": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitest/coverage-v8": "^0.34.2",
    "att-string-transcoder": "^3.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.34.2"
  }
}
